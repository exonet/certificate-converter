{
    "name": "exonet/certificate-converter",
    "description": "PHP package to convert an SSL certificate to various formats (e.g PKC12, PEM).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Exonet B.V.",
            "email": "development@exonet.nl"
        }
    ],
    "require": {
        "php": "~7.1|^8.0",
        "ext-openssl": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.4.0"
    },
    "autoload": {
        "psr-4": {
            "Exonet\\SslConverter\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    }
}
